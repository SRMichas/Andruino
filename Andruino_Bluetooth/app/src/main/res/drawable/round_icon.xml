<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval" >
            <solid android:color="@color/deg1"/>
            <stroke
                android:color="@android:color/white"
                android:width="3dp"/>
            <size
                android:width="80dp"
                android:height="80dp"/>
        </shape>
    </item>
    <item
        android:width="60dp"
        android:height="60dp"
        android:gravity="center"
        android:drawable="@drawable/ic_devices"/>
</layer-list>